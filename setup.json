{
    "name": "aiida-catmap",
    "author": "Sudarshan Vijay",
    "author_email": "vijays@fysik.dtu.dk",
    "description": "AiiDA package that interfaces with Kinetic modelling code CatMAP",
    "url": "https://github.com/sudarshanv01/aiida-catmap",
    "license": "MIT",
    "classifiers": [
        "Programming Language :: Python",
        "Intended Audience :: Science/Research",
        "License :: OSI Approved :: MIT License",
        "Natural Language :: English",
        "Framework :: AiiDA"
    ],
    "version": "0.2.0a0",
    "entry_points": {
        "aiida.calculations": [
            "catmap = aiida_catmap.calculations:CatMAPCalculation"
        ],
        "aiida.parsers": [
            "catmap = aiida_catmap.parsers:CatMAPParser"
        ]
    },
    "setup_requires": ["reentry"],
    "install_requires": [
        "aiida-core>=1.1.0,<2.0.0",
        "six",
        "voluptuous"
    ],
    "extras_require": {
        "test": [
            "pgtest",
            "wheel",
            "coverage",
            "pytest",
            "pytest-cov",
            "pytest-regressions"
        ],
        "pre-commit": [
            "pre-commit",
            "pylint"
        ],
        "docs": [
            "sphinx",
            "sphinxcontrib-contentui",
            "sphinxcontrib-details-directive; python_version>='3.0'",
            "sphinx-rtd-theme"
        ]
    }
}
